{
    "title": "personal config",
    "rules": [
        {
            "description": "personal config",
            "manipulators": [
                {
                    "from": {
                        "key_code": "caps_lock",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_shift",
                            "modifiers": [
                                "right_command",
                                "right_control",
                                "right_option"
                            ]
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "escape"
                        }
                    ],
                    "type": "basic"
                },

                {
                    "from": {
                        "key_code": "h",
                        "modifiers": {
                            "mandatory": [
                                "right_command",
                                "right_control",
                                "right_shift",
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "/usr/local/bin/yabai -m window --focus west"
                        }
                    ],
                    "type": "basic"
                },

                {
                    "from": {
                        "key_code": "j",
                        "modifiers": {
                            "mandatory": [
                                "right_command",
                                "right_control",
                                "right_shift",
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "/usr/local/bin/yabai -m window --focus south || /usr/local/bin/yabai -m display --focus 2"
                        }
                    ],
                    "type": "basic"
                },

                {
                    "from": {
                        "key_code": "k",
                        "modifiers": {
                            "mandatory": [
                                "right_command",
                                "right_control",
                                "right_shift",
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "/usr/local/bin/yabai -m window --focus north || /usr/local/bin/yabai -m display --focus 1"
                        }
                    ],
                    "type": "basic"
                },

                {
                    "from": {
                        "key_code": "l",
                        "modifiers": {
                            "mandatory": [
                                "right_command",
                                "right_control",
                                "right_shift",
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "/usr/local/bin/yabai -m window --focus east"
                        }
                    ],
                    "type": "basic"
                },

                {
                    "from": {
                        "key_code": "h",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "right_command",
                                "right_control",
                                "right_shift",
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "/usr/local/bin/yabai -m window --swap west || (/usr/local/bin/yabai -m window --space prev; /usr/local/bin/yabai -m space --focus prev)"
                        }
                    ],
                    "type": "basic"
                },

                {
                    "from": {
                        "key_code": "j",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "right_command",
                                "right_control",
                                "right_shift",
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "/usr/local/bin/yabai -m window --swap south || (/usr/local/bin/yabai -m window --display 2; /usr/local/bin/yabai -m display --focus 2)"
                        }
                    ],
                    "type": "basic"
                },

                {
                    "from": {
                        "key_code": "k",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "right_command",
                                "right_control",
                                "right_shift",
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "/usr/local/bin/yabai -m window --swap north || (/usr/local/bin/yabai -m window --display 1; /usr/local/bin/yabai -m display --focus 1)"
                        }
                    ],
                    "type": "basic"
                },

                {
                    "from": {
                        "key_code": "l",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "right_command",
                                "right_control",
                                "right_shift",
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "/usr/local/bin/yabai -m window --swap east || (/usr/local/bin/yabai -m window --space next; /usr/local/bin/yabai -m space --focus next)"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
